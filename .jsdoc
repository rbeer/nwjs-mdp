{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["package.json", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|public/static/vendor|docs/)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "collapseSymbols": false,
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "theme": "paper",
        "linenums": true
    },
    "opts": {
        "destination": "./docs/jsdoc/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/minami"
    }
}
